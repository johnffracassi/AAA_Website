<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'layouts/header' %> <!-- renders partial called header -->
<%= render 'layouts/navigation' %> 
<%= yield %> <!-- The contents of the respective view file is evaluated and inserted here -->
<%= render 'layouts/footer' %> 

</body>
</html>
